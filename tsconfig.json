{
	"compilerOptions": {
		"target": "ES6",
		"module": "esnext",
		"jsx": "react-jsx",
		"strict": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"allowJs": true,
		"noEmit": true,
		"types": ["jest", "@testing-library/jest-dom"]
	},
	"include": ["src"]
}
